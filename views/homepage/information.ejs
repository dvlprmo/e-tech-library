<div class="boardBox">

<div class="newNoneClass" style="width: 18rem;">
    
    <% //books.forEach(book =>{ %>
        <div class="alone">
            <img class="imageOm" src="<%= book.image%>" alt="Card image cap">
        </div>
   
    <div class="textOM">
      <h4 class="textNum"> TITLE: <%= book.title%> </h4>
      <h4 class="textNum"> ADDED ON: <%= moment(book.createdAt).fromNow()%></h4>
      <h4 class="textNum"> PUBLISHER: <%= book.publisher%></h4>
      <h4 class="textNum"> ISBN: <%= book.ISBN%></h4>
      <h4 class="textNum"> PAGE NUMBERS: <%= book.numberOfPage%></h4>
      <h4 class="textNum"> LINK: <%= book.link%></h4>
    </div>  
    <% // }) %>
    </div>

    <div class="buttonReview">
        <a  href="/homepage/readlist/<%= book._id %>" type="submit" name="readlist" class="btn btn-info signBut5" role="button" value="readlist">ALREADY READ? </a>
    </div>
      <div class="moreInfo">
        <a  href="/homepage/bookfav/<%= book._id %>" type="submit" name="moreInfo" class="btn btn-info signBut1" role="button" value="moreInfo">ADD TO FAVORITE LIST</a>
    </div>
  </div>

</div>

  