
  <div class="containerABC">
    <% books.forEach(book => { %>
      
      <div class="col-4">
        <div class="box">
          <div class="box-body">
            <h6 class="col-sm box-subtitle mb-2 text-muted"> <%= book.title %></h6>
            <p class="box-text">
              <img src="<%= book.image %>" alt="BookImage" style="width: 90px; height: 90px;"/></div>
            </p>
            <div class="moreInfo">
            <form class="inline" method="POST" action="/homepage/readlist/<%= book._id %>/delete?_method=DELETE">
              <button class="btn btn-info signBut1" type="submit">Delete</button>
          </form>
        </div>
    </div>
  
      </div>
      <% })%>
    </div>
  